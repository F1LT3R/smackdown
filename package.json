{
  "name": "smackdown",
  "description": "converts nested directory of markdown into html",
  "keywords":"markdown html",
  "homepage":"http://f1lt3r.com/smackdown",
  "license" : "MIT",
  "author": "Alistair MacDonald <al@pwn.io>",
  "version":"0.1.0",
  "dependencies": {
    "bluebird": "*",
    "minimatch": "*",
    "marked": "*",
    "less": "*",
    "node-terminal": "*",
    "underscore": "*"
  },
  "engine": "node >= 0.4.1",
  "repository" :{
    "type" : "git",
    "url" : "http://github.com/f1lt3r/smackdown.git"
  },
  "scripts": {
    "start": "node ./bin/smackdown.js"
  },
  "config":{
    "input_dir":"./test",
    "html_output_dir":"./html-output",
    "less_stylesheet":"https://f1lt3r.github.io/markdown-css/markdown-default.less"
  }
}